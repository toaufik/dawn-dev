{% render 'header-main' %}

{% schema %}
{
  "name": "Header",
  "settings": [
    {
      "type": "header",
      "content": "Header Main"
    },
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Upload your logo"
    },
    {
      "type": "range",
      "id": "logo_width",
      "label": "Resize your logo",
      "min": 80,
      "max": 300,
      "step": 20,
      "unit": "px",
      "default": 100
    },
    {
      "type": "link_list",
      "id": "main_menu",
      "label": "Select your menu",
      "default": "main-menu"
    }
  ],
  "presets": [
    {
      "name": "Header",
      "category": "Header"
    }
  ]
}
{% endschema %}
